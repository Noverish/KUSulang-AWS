doctype html
html
  include ../head.pug
  body
    - var is_statics = 'active';
    include ../nav.pug
    include ./list_tab.pug
    
    div(class='container')
      table(class='table table-responsive table-hover')
        tr
          th #
          each val, index in results[0]
            th=index
        each val, index in results
          tr
            td=index+1
            each item in results[index]
              td=item
          
